{% extends 'CrousBackendBundle::layout.html.twig' %} 
{% block meta%}
    <title>{{'Assistance Element'|trans}}</title>
    <meta name="description" content="{{'Assistance Element'|trans}}">
    <meta name="keywords" content="{{'Assistance Element'|trans}}">
{% endblock %}
{%block homecls%}class="blue-theme"{%endblock%}
{% block body %}
    {% form_theme form 'CrousBackendBundle:Form:fields.html.twig' %}
    <div class="inner">
        <div class="row">
            <form class="form-default formulaire-form" method="post" action="" enctype="multipart/form-data" novalidate="novalidate">
                <div class="col-xs-12">
                    {% include 'CrousBackendBundle:Block:messages.html.twig' %}
                    <div class="form-body">
                        {{form_widget(form)}}
                    </div>
                </div>
                <div class="col-xs-12 col-sm-9 col-sm-offset-3">
                    <div class="wrap-btn style-3">
                        <button type="submit" class="btn btn-default">{{'Save'|trans}}</button>
                        <a type="submit" href="{{path('crous_backend_assistance_add', {id:pid})}}" class="btn btn-default">{{'Back'|trans}}</a>
                    </div>
                </div>
            </form>
        </div>
    </div>
{% endblock %}

